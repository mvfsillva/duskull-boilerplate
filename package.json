{
  "name": "duskull-boilerplate",
  "version": "1.0.0",
  "author": "Marcus Silva <mvfsillva@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "format": "prettier --write '**/*.js'",
    "format:check": "eslint --print-config .eslintrc.yaml | eslint-config-prettier-check",
    "lint": "eslint --ext .js src",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:watch": "esw --watch --ext .js src",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "coveralls": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls && rm -rf coverage",
    "clean": "rimraf build",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --progress --config webpack/prod.config -p",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvfsillva/duskull-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mvfsillva/duskull-boilerplate/issues"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^src/([^\\.]*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/config/test/enzyme.theme.js",
      "<rootDir>/config/test/fetch.setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/test/enzyme.setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "homepage": "https://github.com/mvfsillva/duskull-boilerplate#readme",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-jest": "^23.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-watch": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "jest-styled-components": "^5.0.1",
    "jsdom": "^11.11.0",
    "nock": "^9.4.2",
    "now": "^11.3.1",
    "prettier": "^1.13.7",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "stylelint": "^9.3.0",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "webpack": "^3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.0.6",
    "webpack-dev-server": "^2.11.2"
  }
}
